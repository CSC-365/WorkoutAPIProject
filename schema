create table
  public.users (
    user_id bigint generated by default as identity not null,
    starting_lbs bigint null,
    name text null,
    height_inches bigint null,
    avg_calorie_intake bigint null,
    age bigint null,
    gender text null,
    constraint users_pkey primary key (user_id)
  ) tablespace pg_default;

create table
  public.goals (
    goal_id bigint generated by default as identity not null,
    user_id bigint null,
    type_id bigint null,
    target_weight bigint null,
    workout_id bigint null,
    constraint goals_pkey primary key (goal_id)
  ) tablespace pg_default;

create table
  public.log (
    log_id bigint generated by default as identity not null,
    user_id bigint null,
    current_lbs bigint null,
    time_posted timestamp without time zone null,
    constraint log_pkey primary key (log_id)
  ) tablespace pg_default;
  
  create table
  public.workouts (
    workout_id bigint generated by default as identity not null,
    workout_name text null,
    weight bigint null,
    distance_ft bigint null,
    repetitions bigint null,
    seconds bigint null,
    sets bigint null,
    times_per_week bigint null,
    user_id bigint null,
    constraint workouts_pkey primary key (workout_id),
    constraint workouts_user_id_fkey foreign key (user_id) references users (user_id)
  ) tablespace pg_default;
  
  create table
  public.goal_types (
    type_id bigint generated by default as identity not null,
    type text null,
    constraint goal_types_pkey primary key (type_id)
  ) tablespace pg_default;
